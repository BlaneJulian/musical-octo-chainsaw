function HSVtoRGB(a){var b,e,g,m,i,n,j;j=a.s;var c=a.v;i=a.h;a=Math.floor(i*6);m=i*6-a;i=c*(1-j);n=c*(1-m*j);j=c*(1-(1-m)*j);switch(a%6){case 0:b=c;e=j;g=i;break;case 1:b=n;e=c;g=i;break;case 2:b=i;e=c;g=j;break;case 3:b=i;e=n;g=c;break;case 4:b=j;e=i;g=c;break;case 5:b=c;e=i;g=n}return{r:Math.round(b*255),g:Math.round(e*255),b:Math.round(g*255)}}
function RGBtoHSV(a){var b=a.r/255,e=a.g/255,g=a.b/255;a=Math.max(b,e,g);var m=Math.min(b,e,g),i=a-m;if(i==0)return{h:0,s:0,v:a};b=60*(b==m?3-(e-g)/i:g==m?1-(b-e)/i:5-(g-b)/i);b/=360;return{h:b,s:i/a,v:a}}function RGBtoHEX(a){return(16777216+(a.r<<16)+(a.g<<8)+a.b).toString(16).slice(1)}function HEXtoRGB(a){return(a=/^#?([a-f\d]{2})([a-f\d]{2})([a-f\d]{2})$/i.exec(a))?{r:parseInt(a[1],16),g:parseInt(a[2],16),b:parseInt(a[3],16)}:null}
function getBrighterHex(a){if(a=="000000")return"222222";if(a=="ffffff")return"eeeeee";var b=HEXtoRGB(a);b=RGBtoHSV(b);if(isDark(a))if(b.s==1&&b.v==1){b.h-=0.025;b.s-=0.125}else if(b.s==0)b.v+=0.075;else{b.h-=0.005;b.s+=0.1;b.v+=0.12}else if(b.s==1&&b.v==1){b.h-=0.025;b.s-=0.125}else if(b.s==0)b.v+=0.125;else{b.h-=0.005;b.s+=0.08;b.v+=0.12}if(b.h<0)b.h=360+b.h;if(b.s>1)b.s=1;if(b.v>1)b.v=1;a=HSVtoRGB(b);return RGBtoHEX(a)}
function getDarkerHex(a){if(a=="000000")return a;if(a=="ffffff")return"dddddd";var b=HEXtoRGB(a);b=RGBtoHSV(b);if(isDark(a)){b.s+=0.04;b.v-=0.1}else{b.s+=0.02;b.v-=0.05}if(b.s>1)b.s=1;if(b.v<0)b.v=0;a=HSVtoRGB(b);return RGBtoHEX(a)}function getLuminance(a){a=[a.r,a.g,a.b].map(function(b){b/=255;return b<=0.03928?b/12.92:Math.pow((b+0.055)/1.055,2.4)});return a[0]*0.2126+a[1]*0.7152+a[2]*0.0722}function isDark(a){a=HEXtoRGB(a);return a.r+a.g+a.b>382?false:true};

